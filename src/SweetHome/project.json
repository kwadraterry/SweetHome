{
    "approot": "app",
    "webroot": "wwwroot",
    "version": "1.0.0-*",
    "dependencies": {
        "Kestrel": "1.0.0-beta4",
        "Microsoft.AspNet.Diagnostics": "1.0.0-beta4",
        "Microsoft.AspNet.Mvc": "6.0.0-beta4",
        "Microsoft.AspNet.Session": "1.0.0-beta4",
        "Microsoft.AspNet.SignalR.Server": "3.0.0-beta4",
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta4",
        "Microsoft.Framework.Caching.Distributed": "1.0.0-beta4",
        "Microsoft.Framework.ConfigurationModel.Json": "1.0.0-beta4",
        "Microsoft.Framework.Logging.Console": "1.0.0-beta4",
        "NHibernate": "4.0.3.4000",
        "FluentNHibernate": "2.0.1.0",
        "Npgsql": "3.0.0-rc0002"
    },
    "commands": {
        "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://0.0.0.0:5004",
        "production": "Microsoft.AspNet.Hosting --server Kestrel --server.urls localhost:5004",
        "gen": "Microsoft.Framework.CodeGeneration",
        "db": "SweetHome"
    },
    "frameworks": {
        "dnx451": { }
    },
    "exclude": [
        "wwwroot",
        "node_modules",
        "app/lib"
    ],
    "packExclude": [
        "node_modules",
        "bower_components",
        "**.kproj",
        "**.user",
        "**.vspscc"
    ],
    "scripts": {
        "postrestore": [ "npm install" ]
    }
}
